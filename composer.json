{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "spatie/laravel-data": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "captainhook/captainhook": "^5.14",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.14",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.4",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "ramsey/conventional-commits": "^1.3",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "fix-style": [
            "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.php"
        ],
        "check-style": [
            "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.php --diff --verbose --dry-run --stop-on-violation"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse --xdebug --memory-limit 512M"
        ],
        "phpcbf": [
            "./vendor/bin/phpcbf --standard=PSR12  src/"
        ],
        "phpcs": [
            "./vendor/bin/phpcs --standard=PSR12  src/"
        ],
        "phpinsights": [
            "php artisan insights -n"
        ],
        "fix-phpinsights": [
            "php artisan insights --fix -n"
        ],
        "cghooks": "vendor/bin/cghooks",
        "test": [
            "vendor/bin/phpunit"
        ],
        "test:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html build/coverage",
        "pipes": [
            "composer update",
            "composer dump",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "composer fix-phpinsights",
            "composer fix-style",
            "composer phpstan",
            "composer phpinsights",
            "composer phpcs",
            "composer phpcbf",
            "composer check-style",
            "composer test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
